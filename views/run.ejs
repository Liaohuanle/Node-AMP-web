
<script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
<script type="text/javascript" src="//cdn.jsdelivr.net/gh/kenwheeler/slick@1.8.1/slick/slick.min.js"></script>
<script src="https://cdn.plyr.io/2.0.18/plyr.js"></script>
<script>
  const timeNode = <%- setIntervalTimeNode %>
  const restTime = timeNode.restTime
  window.onload = function(){
    initTime(...restTime)
    let sec = restTime[3]
    let min = restTime[2]
    let hour = restTime[1]
    let day = restTime[0]
    setInterval(function(){
      sec--
      if( sec <= 0 ){
        sec = 59
        min--
        if(min <= 0){
          min = 59
          hour--
          if(hour <= 0){
            hour = 23
            day--
            if( day <= 0){
              day = 0
            }
          }
        }
      }
      initTime(day, hour, min, sec)
    }, 1000)
  }

  $(function(){
    $(".slider").slick({
      dots: false,
      speed: 500
    })
    $(".slider-meet").slick({
      dots: true,
      speed: 500,
      slidesToShow: 3,
      slidesToScroll: 3,
    })
    plyr.setup($('#muscial_ly_you_tube'), {
      enabled: true,
      autoplay: true
    })
  })

  function initTime(day, hour, min, sec){
    $('.days span').html(day < 10 ? `0${day}`: day)
    $('.hours span').html(hour < 10 ? `0${hour}`: hour)
    $('.minutes span').html(min < 10 ? `0${min}`: min)
    $('.seconds span').html(sec < 10 ? `0${sec}`: sec)
  }

</script>